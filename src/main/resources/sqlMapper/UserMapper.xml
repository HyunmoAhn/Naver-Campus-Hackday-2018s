<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.organogram.UserMapper">
  <select id="list" resultType="com.organogram.User">
    SELECT
    user_id AS 'id',
    user_name AS 'name'
    FROM
    TB_USER
  </select>
</mapper>
